{
  "name": "cces",
  "version": "1.0.0",
  "description": "cces",
  "main": "build/index.js",
  "scripts": {
    "check": "npx @biomejs/biome check --apply ./src",
    "dev": "npm run spec:routes && concurrently \"nodemon\" \"nodemon -x tsoa spec-and-routes\"",
    "prisma:generate": "npx prisma generate",
    "build": "npm run prisma:generate && tsoa spec-and-routes && tsc",
    "start": "node src/index.js",
    "spec:routes": "npx tsoa spec-and-routes",
    "migrate": "npx prisma migrate dev",
    "seed": "ts-node ./prisma/seeds/index.ts",
    "reset": "npx prisma migrate reset",
    "lint": "npx eslint ./src --fix ",
    "format": "prettier --write ./src",
    "prepare": "husky install",
    "lint-staged": "lint-staged",
    "build:dev": "tsc --outDir dist-dev"
  },
  "engines": {
    "node": "22.12.0",
    "pnpm": "Please use npm ",
    "npm": ">= 9",
    "yarn": "PLease use npm"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@biomejs/biome": "1.7.3",
    "@commitlint/cli": "^19.3.0",
    "@eslint/js": "^9.4.0",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node-schedule": "^2.1.7",
    "@types/nodemailer": "^6.4.16",
    "@types/swagger-ui-express": "^4.1.6",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.13.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "globals": "^15.4.0",
    "husky": "^8.0.0",
    "lint-staged": "^15.2.6",
    "nodemon": "^3.1.0",
    "prettier": "^3.3.2",
    "rimraf": "^5.0.7",
    "ts-node": "^10.9.2",
    "tsoa": "^6.2.1",
    "typescript": "^5.4.5",
    "typescript-eslint": "^7.13.0"
  },
  "dependencies": {
    "@dev-heir/express-logs": "^1.0.2",
    "@prisma/client": "^5.15.0",
    "@tsoa/runtime": "^6.2.1",
    "@types/multer": "^1.4.11",
    "axios": "^1.7.2",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dayjs": "^1.11.12",
    "dotenv": "^16.4.5",
    "express": "^4.19.2",
    "jsonwebtoken": "^9.0.2",
    "multer": "^1.4.3",
    "multer-storage-cloudinary": "^4.0.0",
    "node-schedule": "^2.1.1",
    "nodemailer": "^6.9.15",
    "prisma": "^5.14.0",
    "read-excel-file": "^5.8.2",
    "swagger-ui-express": "^5.0.0",
    "uuid": "^9.0.1",
    "zod": "^3.23.8"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint ./src --fix",
      "prettier --write .",
      "git add ."
    ]
  }
}
